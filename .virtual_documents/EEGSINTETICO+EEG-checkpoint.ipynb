        rms_emg = get_rms(emg_section)
        
        SNR_dB = 0 
        SNR = 10 ** (SNR_dB / 10)
        
        coe = rms_eeg / (rms_emg * SNR)
        
        emg_scaled = emg_section * coe * emg_scale
        
        EEG['data'][chan, i_initial:i_final, triali] += emg_scaled

